    "name": "dapploy-frontend",
    "version": "2.0.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@ethersproject/abi": "^5.6.4",
        "@ethersproject/address": "^5.6.1",
        "@ethersproject/bignumber": "^5.6.2",
        "@ethersproject/bytes": "^5.6.1",
        "@ethersproject/constants": "^5.6.1",
        "@ethersproject/contracts": "^5.6.2",
        "@ethersproject/providers": "^5.6.8",
        "@ethersproject/strings": "^5.6.1",
        "@ethersproject/units": "^5.6.1",
        "@mui/icons-material": "^5.8.2",
        "@mui/lab": "^5.0.0-alpha.85",
        "@mui/material": "^5.8.2",
        "@mui/x-date-pickers": "^5.0.0-beta.4",
        "@rainbow-me/rainbowkit": "^0.8.1",
        "@wagmi/core": "^0.8.14",
        "@web3-react/core": "^6.1.9",
        "axios": "^0.27.2",
        "bignumber.js": "^9.0.2",
        "date-fns": "^2.29.1",
        "ethers": "^5.7.2",
        "husky": "^8.0.1",
        "lodash": "^4.17.21",
        "onchange": "^7.1.0",
        "react": "^18.1.0",
        "react-app-rewired": "^2.2.1",
        "react-copy-to-clipboard": "^5.1.0",
        "react-device-detect": "^2.2.2",
        "react-dom": "^18.1.0",
        "react-jazzicon": "^1.0.4",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "react-transition-group": "^4.4.2",
        "run-script-os": "^1.1.6",
        "typescript": "^4.4.2",
        "wagmi": "^0.10.10"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "eject": "react-app-rewired eject",
        "lint": "eslint . --ext .tsx,.ts",
        "postinstall": "husky install",
        "format": "run-script-os",
        "format:win32": "prettier --config .prettierrc \"./src/**/*.{tsx,ts}\" --write",
        "format:darwin:linux": "prettier --config .prettierrc 'src/**/*.{tsx,ts}' --write",
        "format:default": "prettier --config .prettierrc 'src/**/*.{tsx,ts}' --write",
        "watch": "run-script-os",
        "watch:win32": "onchange \"src/**/*.{tsx,ts}\" -- prettier --write {{changed}}",
        "watch:darwin:linux": "onchange 'src/**/*.{tsx,ts}' -- prettier --write {{changed}}",
        "watch:default": "onchange 'src/**/*.{tsx,ts}' -- prettier --write {{changed}}"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run format && yarn run lint"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^28.1.0",
        "@types/lodash": "^4.14.182",
        "@types/node": "^17.0.39",
        "@types/react": "^18.0.11",
        "@types/react-dom": "^18.0.5",
        "@types/react-transition-group": "^4.4.4",
        "@types/webpack-env": "^1.17.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "assert": "^2.0.0",
        "buffer": "^6.0.3",
        "crypto": "^1.0.1",
        "crypto-browserify": "^3.12.0",
        "eslint": "^8.17.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "fs": "^0.0.1-security",
        "https-browserify": "^1.0.0",
        "lint-staged": "^13.0.2",
        "os-browserify": "^0.3.0",
        "prettier": "^2.6.2",
        "process": "^0.11.10",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "url": "^0.11.0"
    }
}
